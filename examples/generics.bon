def identity(x):
  return x

def add(x, y):
  z = x + y
  return z

print("Calling 'identity' with a number: " ++ str(identity(1)))
print("Calling 'identity' with a string: " ++ identity("hello!"))
print("Calling 'add' with integers: " ++ str(add(5, 10)))
print("Calling 'add' with floats: " ++ str(add(19.57, 20.18)))
