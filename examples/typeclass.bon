class Norm<T>
  def norm(v:T) -> float;
end

type point2d
  Point2D(x:float, y:float)
end

impl Norm<point2d>
  def norm(v:point2d) -> float
    sqrt(v.x * v.x + v.y * v.y)
  end
end

type point3d
  Point3D(x:float, y:float, z:float)
end

impl Norm<point3d>
  def norm(v:point3d) -> float
    sqrt(v.x * v.x + v.y * v.y + v.z * v.z)
  end
end

def main()
  print("Test of typeclasses:")
  p = Point2D(2.0, 1.5)
  print(norm(p))
  p2 = Point3D(2.0, 1.5, 3.0)
  print(norm(p2))
end

main()
